{
  "family": "multi-agent-system",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::747674211436:role/multi-agent-system-ecs-task-execution-role",
  "taskRoleArn": "arn:aws:iam::747674211436:role/multi-agent-system-ecs-task-role",
  "containerDefinitions": [
    {
      "name": "multi-agent-system",
      "image": "747674211436.dkr.ecr.us-east-1.amazonaws.com/multi-agent-system:latest",
      "portMappings": [
        {
          "containerPort": 8000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/multi-agent-system",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "orchestrator"
        }
      },
      "environment": [
        {
          "name": "AWS_REGION",
          "value": "us-east-1"
        },
        {
          "name": "ENVIRONMENT",
          "value": "dev"
        }
      ],
      "secrets": [
        {
          "name": "ANTHROPIC_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:747674211436:secret:multi-agent-system-api-keys-z6sbEy:ANTHROPIC_API_KEY::"
        }
      ],
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "curl -f http://localhost:8000/health || exit 1"
        ],
        "interval": 30,
        "timeout": 5,
        "retries": 3,
        "startPeriod": 60
      }
    }
  ]
}
